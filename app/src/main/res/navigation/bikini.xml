<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bikini_navigation"
    app:startDestination="@id/bikini">

    <fragment
        android:id="@+id/bikini"
        android:name="com.example.bikini_android.ui.map.BikiniMapFragment"
        android:label="@string/bikini_toolbar_title"
        tools:layout="@layout/fragment_bikini_map">
        <action
            android:id="@+id/action_bikini_map_to_grid_feeds_end"
            app:destination="@id/grid_feeds_end" />
    </fragment>
    <fragment
        android:id="@+id/grid_feeds_end"
        android:name="com.example.bikini_android.ui.feeds.FeedsFragment"
        tools:layout="@layout/fragment_feeds">
        <action
            android:id="@+id/action_grid_feeds_end_to_linear_feeds_end"
            app:destination="@id/linear_feeds_end" />
    </fragment>
    <fragment
        android:id="@+id/linear_feeds_end"
        android:name="com.example.bikini_android.ui.feeds.FeedsFragment"
        tools:layout="@layout/fragment_feeds"/>
</navigation>
